;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; reverse record
    IN NS p330.nextlab.
    IN NS dell-precision.nextlab.

47 IN PTR p330.nextlab. ; p330
108 IN PTR dell-precision.nextlab. ; dell_precision
